\n  };\n  module.exports = fromCodePoint;\n}\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./~/commonmark/lib/from-code-point.js\n// module id = 757\n// module chunks = 0\n//# sourceURL=webpack:///./~/commonmark/lib/from-code-point.js?");

